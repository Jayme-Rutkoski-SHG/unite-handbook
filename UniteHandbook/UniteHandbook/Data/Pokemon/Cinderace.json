{
    "Name": "Cinderace",
    "AttackStyle": "Attack",
    "Role": "Attacker",
    "Style": "Ranged",
    "Difficulty": "Novice",
    "Ability": [
        {
            "Name": "Blaze",
            "Desc": "When at half HP or less, critical-hit rate is increased by 10% and auto attack speed is increased by 20% for 5s (30s cooldown). When hitting an enemy with an auto attack, a cinder is placed on that enemy for 5s. Boosted Attacks and moves apply 2 cinders. When the target receives it's 5th cinder, they will all be consumed to deal additional damage to the enemy./n/nAgainst Wild Pokemon: The total damage is capped at 500."
        }
    ],
    "Moves": [
        {
            "Name": "Ember",
            "Category": "Sure Hit",
            "Cooldown": 6.00,
            "LevelDetails": [
                {
                    "Level": 1,
                    "Desc": "Cinderace shoots out a small flame, damaging and applying burn to hit enemies. The burn lasts 3s ticking damage once every second and reduces enemy Attack and Sp. Attack by 5% for 2s."
                }
            ],
            "Upgrades": [
                {
                    "Name": "Pyro Ball",
                    "Category": "Ranged",
                    "Cooldown": 5.00,
                    "LevelDetails": [
                        {
                            "Level": 7,
                            "Desc": "Cinderace kicks a ball of flame in a designated direction, burning and damaging enemies hit. The burn lasts 3s ticking damage once every second and reduces enemy Attack and Sp. Attack by 5% for 2s."
                        },
                        {
                            "Level": 11,
                            "Desc": "Reduces the cooldown of Flame Charge or Feint by 3.5s when this move hits."
                        }
                    ]
                },
                {
                    "Name": "Blaze Kick",
                    "Category": "Sure Hit",
                    "Cooldown": 8.00,
                    "LevelDetails": [
                        {
                            "Level": 7,
                            "Desc": "Perform a fiery flying kick, jumping towards a designated enemy, shoving and burning them. Cinderace can land in a targeted direction after this move hits. In addition, Cinderace is immune to hindrances during this move and is always a critical-hit. The burn lasts 3s ticking damage once every second and reduces enemy Attack and Sp. Attack by 5% for 2s."
                        },
                        {
                            "Level": 11,
                            "Desc": "Increased auto attack speed by 65% for 3s after this move hits."
                        }
                    ]
                }
            ]
        },
        {
            "Name": "Low Sweep",
            "Category": "Dash",
            "Cooldown": 7,50,
            "LevelDetails": [
                {
                    "Level": 1,
                    "Desc": "Cinderace makes a swift low sweep in the designated direction, damaging any enemy it hits."
                }
            ],
            "Upgrades": [
                {
                    "Name": "Flame Charge",
                    "Category": "Dash",
                    "Cooldown": 5.00,
                    "LevelDetails": [
                        {
                            "Level": 8,
                            "Desc": "Cinderace charges forward, damaging all enemies hit."
                        },
                        {
                            "Level": 13,
                            "Desc": "The next boosted attack after this move ends also slows enemies hit by 30% for 2s."
                        }
                    ]
                },
                {
                    "Name": "Feint",
                    "Category": "Buff",
                    "Cooldown": 11.00,
                    "LevelDetails": [
                        {
                            "Level": 8,
                            "Desc": "Cinderace dodges, increasing movement speed by 25% for 2s and becoming briefly invincible for 44 frames (.73s)."
                        },
                        {
                            "Level": 13,
                            "Desc": "After dodging, the next 3 auto attacks that hit restore HP equal to 30% of damage dealt."
                        }
                    ]
                }
            ]
        }
    ],
    "Unite": [
        {
            "Name": "Blazing Bicycle Kick",
            "Category": "Sure Hit",
            "Cooldown": 150,
            "LevelDetails": [
                {
                    "Level": 9,
                    "Desc": "Create a giant ball of flame and kick it toward the designated location. The ball releases a fierce shock wave that deals damage to enemies in the area of effect. Also increases movement speed by 10% (stacks with the Unite Buffs) for 5s when it hits."
                }
            ]
        }
    ],
