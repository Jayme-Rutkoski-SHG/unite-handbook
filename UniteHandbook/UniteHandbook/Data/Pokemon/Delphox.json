{
    "Name": "Delphox",
    "AttackStyle": "Special",
    "Role": "Attacker",
    "Style": "Ranged",
    "Difficulty": "Novice",
    "Ability": [
        {
            "Name": "Blaze",
            "Desc": "When using a move when below 50% max HP, that move deals additional damage with a fireball to enemies. This ability goes on a 15s cooldown when triggered."
        }
    ],
    "Moves": [
        {
            "Name": "Ember",
            "Category": "Ranged",
            "Cooldown": 6.00,
            "LevelDetails": [
                {
                    "Level": 1,
                    "Desc": "Shoot a small flame in front of itself, dealing damage to enemies hit."
                }
            ],
            "Upgrades": [
                {
                    "Name": "Fire Blast",
                    "Category": "Ranged",
                    "Cooldown": 6.00,
                    "LevelDetails": [
                        {
                            "Level": 6,
                            "Desc": "Delphox launches an intense, all-consuming fire in a forward direction; dealing damage to enemies hit. As the fire travels, it leaves behind a zone of flames in which a delayed explosion will occur; dealing damage to enemies in the area-of-effect."
                        },
                        {
                            "Level": 11,
                            "Desc": "Damage increased."
                        }
                    ]
                },
                {
                    "Name": "Mystical Fire",
                    "Category": "Ranged",
                    "Cooldown": 4.50,
                    "LevelDetails": [
                        {
                            "Level": 6,
                            "Desc": "Delphox unleashes a special, hot ball of fire in a forward direction. If it hits an enemy, the fire will explode; dealing damage to enemies in the area-of-effect."
                        },
                        {
                            "Level": 11,
                            "Desc": "Also reduces cooldowns of all moves if this move hits by 1s. If it hits an enemy from the opposing team, cooldowns are reduced even further (1.5s)."
                        }
                    ]
                }
            ]
        },
        {
            "Name": "Will-o-Wisp",
            "Category": "Area",
            "Cooldown": 7.00,
            "LevelDetails": [
                {
                    "Level": 1,
                    "Desc": "Conjure up 3 sinister flames and launch them toward the designated area. These flames then explode, dealing damage to enemies hit by the explosion and decreasing their movement speed by 30% for 2s. Subsequent flames will deal 50% less to the same target. An enemy will only be counted as being hit twice, even if all 3 flames hit."
                }
            ],
            "Upgrades": [
                {
                    "Name": "Fire Spin",
                    "Category": "Area",
                    "Cooldown": 8.00,
                    "LevelDetails": [
                        {
                            "Level": 7,
                            "Desc": "Create a vortex of fire in the designated area. The vortex automatically moves toward the nearest enemy, dealing damage-over-time to all enemies caught in the area for a short time and decreasing their movement speed by 30% for 5s. After 4.5s passes or an enemy is caught in the center of the vortex; the flames increase in intensity. This increases the rate at which the flames deal damage and also leaves the enemies unable to act for 1s. This trapped damage ticks four times total. The Fire Spin will otherwise deal damage as it travels. Move placement on top of enemies will apply a \"searching\" tick and then apply \"trapped\" damage. A maximum of 9 ticks is possible."
                        },
                        {
                            "Level": 13,
                            "Desc": "Applies a debuff on trap activation that increases all damage dealt by Delphox by 15% while enemies are trapped. Debuff is only cleansable after the first tick of trap damage and does not re-apply the debuff."
                        }
                    ]
                },
                {
                    "Name": "Flame Charge",
                    "Category": "Dash",
                    "Cooldown": 4.50,
                    "LevelDetails": [
                        {
                            "Level": 7,
                            "Desc": "Delphox charges forward in the designated direction. If there are any enemies within auto attack range when the charge ends, summon three flames to attack. These flames deal damage and decrease enemy movement speed by 30% for 2s. This move can be used while also using Fire Blast or Mystical Fire. Each subsequent flame that hits deals 70% less."
                        },
                        {
                            "Level": 13,
                            "Desc": "Increases Delphox's movement speed by 75% for 1s when this move is used."
                        }
                    ]
                }
            ]
        }
    ],
    "Unite": [
        {
            "Name": "Fanciful Fireworks",
            "Category": "Area",
            "Cooldown": 84,
            "LevelDetails": [
                {
                    "Level": 9,
                    "Desc": "Deals damage-over-time to enemies in the designated area for 8s. Enemies that are hit have 50% decreased movement speed and 50% reduced HP recovery effects for 1.75s. Recovery effects are weakened within the zone and cannot be cleansed while in the zone. Leaving the zone will have the weakened HP recovery effects linger for 1.75s and may be cleansed after leaving the zone. 8s duration (ticks at 0.5s intervals)."
                }
            ]
        }
    ],
