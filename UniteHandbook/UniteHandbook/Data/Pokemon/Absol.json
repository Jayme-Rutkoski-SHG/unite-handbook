    "Moves": [
        {
            "Name": "Feint",
            "Category": "Dash",
            "Cooldown": 6.40,
            "LevelDetails": [
                {
                    "Level": 1,
                    "Desc": "Absol dashes in an arc, damaging all enemies hit. Each use alternates between a left and right arc. Feint ignores the target’s Defense and bypasses all shields."
                }
            ],
            "Upgrades": [
                {
                    "Name": "Night Slash",
                    "Category": "Dash",
                    "Cooldown": 4.00,
                    "LevelDetails": [
                        {
                            "Level": 5,
                            "Desc": "Absol dashes in an arc, damaging the first Pokémon hit. If Absol hits, this ability can be activated within 6s – allowing Absol to dash again, dealing damage to all enemies hit and increasing attack speed once again. The second hit also has 30% increased Critical-Hit Rate on enemies below 50% max HP. \n\nCooldown on Hit or without recasting is 6s."
                        },
                        {
                            "Level": 11,
                            "Desc": "Absol heals for 50% of damage dealt on the second hit, including critical hits."
                        }
                    ]
                },
                {
                    "Name": "Pursuit",
                    "Category": "Dash",
                    "Cooldown": 4.00,
                    "LevelDetails": [
                        {
                            "Level": 5,
                            "Desc": "Absol dashes to a location, and dashes to an enemy with its next auto attack. If Absol hits the back of the target, this ability's cooldown is reduced by 80%. If it fails to hit the back, damage is reduced. The next auto attack stays empowered for 3s."
                        },
                        {
                            "Level": 11,
                            "Desc": "Absol gains 35% increased movement speed for 2s after the initial dash."
                        }
                    ]
                }
            ]
        },
        {
            "Name": "Slash",
            "Category": "Melee",
            "Cooldown": 6.00,
            "LevelDetails": [
                {
                    "Level": 1,
                    "Desc": "Absol quickly slashes in front of themself, damaging any enemy hit by the slash. Slash has an increased critical-hit rate by 15%."
                }
            ],
            "Upgrades": [
                {
                    "Name": "Psycho Cut",
                    "Category": "Ranged",
                    "Cooldown": 7.00,
                    "LevelDetails": [
                        {
                            "Level": 7,
                            "Desc": "Throw a wide slash forward; damaging and slowing enemies hit by 90% for 2s. This slow decreases by 25% every 0.5s. Enemies hit are marked for 5s with 3 cleansable marks. When Absol deals damage to a marked enemy, a mark is consumed, dealing extra damage. All damage dealt by Psycho Cut or its marks have a 15% increased Critical-Hit Rate."
                        },
                        {
                            "Level": 13,
                            "Desc": "Increases Absol's movement speed by 20% when near a debuffed enemy."
                        }
                    ]
                },
                {
                    "Name": "Sucker Punch",
                    "Category": "Sure Hit",
                    "Cooldown": 6.00,
                    "LevelDetails": [
                        {
                            "Level": 7,
                            "Desc": "Absol targets an enemy and enters a counter stance for 1s, decreasing the movement speed of the target enemy and Absol by 50% for the duration and negating all damage and effects of the next attack on Absol. While in counter stance, if Absol is attacked or the target attacks, Absol immediately exits counter stance and leaps behind the target, stunning it and dealing damage. If the attack is not triggered before the counter stance expires, Absol leaps behind the target, stunning them and dealing damage."
                        },
                        {
                            "Level": 11,
                            "Desc": "Increases Attack Speed by 40% for 3s after using this move."
                        }
                    ]
                }
            ]
        }
    ],
